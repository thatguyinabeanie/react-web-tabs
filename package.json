{
  "name": "@thatguyinabeanie/react-web-tabs",
  "version": "0.1.0",
  "description": "Modular and accessible React tabs component",
  "main": "dist/react-web-tabs.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "es/**/*.js",
    "es/**/*.d.ts",
    "es/**/*.d.ts.map",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.d.ts.map",
    "!**/__tests__",
    "!**/*.test.*"
  ],
  "author": "Marcus Lindfeldt",
  "homepage": "https://github.com/thatguyinabeanie/react-web-tabs",
  "bugs": {
    "url": "https://github.com/thatguyinabeanie/react-web-tabs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thatguyinabeanie/react-web-tabs.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "react",
    "tabs",
    "a11y"
  ],
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "testEnvironment": "jsdom",
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "clean": "rm -rf dist es lib coverage dist-tsc",
    "build": "npm run build:types && npm run build:es && npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run build:styles && npm run build:styles:min",
    "build:types": "tsc --project tsconfig.json",
    "build:es": "babel src -d es --ignore __tests__ --extensions .ts,.tsx && cp dist-tsc/*.d.ts es/ && cp dist-tsc/*.d.ts.map es/",
    "build:commonjs": "BABEL_ENV=commonjs babel src -d lib --ignore __tests__ --extensions .ts,.tsx && cp dist-tsc/*.d.ts lib/ && cp dist-tsc/*.d.ts.map lib/",
    "build:umd": "NODE_ENV=production webpack --mode production",
    "build:umd:min": "NODE_ENV=production webpack --mode production --env minify=true",
    "build:styles": "postcss -o dist/react-web-tabs.css styles/*.css",
    "build:styles:min": "NODE_ENV=production postcss -o dist/react-web-tabs.min.css styles/*.css",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --max-warnings 0",
    "tsc": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "react-compiler-runtime": "^1.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "bash -c 'tsc --noEmit'"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "zod-validation-error": "^4.0.0"
  }
}
